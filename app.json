{
  "expo": {
    "name": "Q's Sneaker",
    "slug": "App",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "qssneaker",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode":"contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.yenmaiteam.qssneaker",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "*.qssneaker.herokuapp.com"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    }
  }
}
